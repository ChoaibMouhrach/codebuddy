{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "preinstall": "npx only-allow pnpm",
    "lint": "next lint",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix --ext .ts,.tsx --cache",
    "prepare": "husky install",
    "prettier:test": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "start": "next start",
    "typecheck": "tsc --noEmit"
